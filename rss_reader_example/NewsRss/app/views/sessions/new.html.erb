<h1>Sign In</h1>
<form action="<%=session_url%>" method="post">
  <input type="hidden" name="authenticity_token" value="<%=form_authenticity_token%>">

  <label>Username
    <input type = "text" name="users[username]" value = ""></label>
  <label>Password
    <input type = "text" name="users[password]" value = ""></label>
  <input type = "submit" value="Sign In">
</form>
